{
  "version": 2,
  "rewrites": [
    { "source": "/scram/:path*", "destination": "/node_modules/@mercuryworkshop/scramjet/:path*" },
    { "source": "/epoxy/:path*", "destination": "/node_modules/@mercuryworkshop/epoxy-transport/:path*" },
    { "source": "/baremux/:path*", "destination": "/node_modules/@mercuryworkshop/bare-mux/:path*" },
    { "source": "/:path*", "destination": "/public/:path*" }
  ]
}
